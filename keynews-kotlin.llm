            when (menuItem.itemId) {
                R.id.nav_news_sources -> {
                    loadFragment(NewsSourcesFragment())
                    true
                }
                R.id.nav_reading_feeds -> {
                    loadFragment(ReadingFeedsFragment())
                    true
