        override fun onReceive(context: Context, intent: Intent) {
            when (intent.action) {
                TtsReadingService.ACTION_READING_PROGRESS -> {
                    val link = intent.getStringExtra(TtsReadingService.EXTRA_ARTICLE_LINK)
                    highlightCurrentlyReading(link)
                }
                TtsReadingService.ACTION_READING_DONE -> {
                    // Maybe show a toast or refresh the list again
                    loadArticles()
                }
            }
        }
    }

    private fun startManualTtsReading() {
        val intent = Intent(requireContext(), TtsReadingService::class.java)
        // If you have a feed ID, pass it. For now, weâ€™ll do feed = 1
        intent.putExtra(TtsReadingService.EXTRA_FEED_ID, 1L)
        // Headlines limit:
        intent.putExtra(TtsReadingService.EXTRA_HEADLINES_LIMIT, 10)
        // whether to read body
