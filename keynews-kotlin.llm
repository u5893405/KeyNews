package com.example.keynews.services

import android.app.Notification
import android.app.NotificationChannel
import android.app.NotificationManager
import android.app.Service
import android.content.Context
import android.content.Intent
import android.os.IBinder
import android.speech.tts.TextToSpeech
import androidx.core.app.NotificationCompat
import androidx.localbroadcastmanager.content.LocalBroadcastManager
import com.example.keynews.KeyNewsApp
import com.example.keynews.data.model.NewsArticle
import com.example.keynews.data.model.ReadingFeed
import kotlinx.coroutines.*
import java.util.Locale

/**
 * TtsReadingService is a ForegroundService that reads news articles
 * using TTS. It broadcasts progress updates so the UI can reflect
 * the currently reading article.
 */
class TtsReadingService : Service(), TextToSpeech.OnInitListener {

    companion object {
        private const val NOTIF_CHANNEL_ID = "TTS_CHANNEL"
        private const val NOTIF_ID = 12345

        // Extras in the Intent that starts the Service
        const val EXTRA_FEED_ID = "extra_feed_id"
        const val EXTRA_HEADLINES_LIMIT = "extra_headlines_limit"
        const val EXTRA_READ_BODY = "extra_read_body"

        // Broadcast Actions
        const val ACTION_READING_PROGRESS = "com.example.keynews.ACTION_READING_PROGRESS"
        const val ACTION_READING_DONE = "com.example.keynews.ACTION_READING_DONE"
        const val EXTRA_ARTICLE_LINK = "extra_article_link"
    }

    private var tts: TextToSpeech? = null

    // We'll use a job + scope for coroutines
    private val serviceJob = Job()
    private val serviceScope = CoroutineScope(Dispatchers.IO + serviceJob)

    private var articlesToRead: List<NewsArticle> = emptyList()
    private var currentIndex = 0
    private var readBody = false

    override fun onCreate() {
        super.onCreate()
        createNotificationChannel()
        startForeground(NOTIF_ID, buildNotification("Preparing TTS..."))

        // Init TTS
        tts = TextToSpeech(applicationContext, this)
    }

    override fun onInit(status: Int) {
        if (status == TextToSpeech.SUCCESS) {
            tts?.language = Locale.getDefault()
        }
        // We proceed to fetch feed articles in the background
        startReading()
    }

    /**
     * We'll gather articles from the feed and start reading them in sequence.
     */
    private fun startReading() {
        serviceScope.launch {
            val feedId = intent.getLongExtra(EXTRA_FEED_ID, -1L)
            val limit = intent.getIntExtra(EXTRA_HEADLINES_LIMIT, 10)
            readBody = intent.getBooleanExtra(EXTRA_READ_BODY, false)

            // Load feed articles from DB
            val db = (application as KeyNewsApp).database
            val feedDao = db.readingFeedDao()
            val articleDao = db.newsArticleDao()

            // If feedId is valid, get the associated sources from cross-ref:
            val sourceRefs = db.readingFeedDao().getSourceIdsForFeed(feedId)
            val sourceIds = sourceRefs.map { it.sourceId }
            // Then get articles from those sources, sorted by time desc
            // (We'll just fetch them all, filter out read if we want, or let limit handle it.)
            val allArticles = if (sourceIds.isNotEmpty()) {
                articleDao.getArticlesBySourceIds(sourceIds)
            } else {
                emptyList()
            }

            // Here you'd normally also apply keyword rules if needed.
            // For brevity, let's skip. Or if user wants the same filtering, do it here.
            // We'll just take the top N articles that are not read:
            articlesToRead = allArticles.filter { !it.isRead }.take(limit)

            if (articlesToRead.isEmpty()) {
                // broadcast done immediately
                broadcastDone()
                stopSelf()
                return@launch
            }

            // Update notification
            updateNotification("Reading ${articlesToRead.size} articles...")

            readArticlesSequentially()
        }
    }

    /**
     * Reads each article in sequence using TTS, marks it read, and broadcasts progress.
     */
    private suspend fun readArticlesSequentially() {
        withContext(Dispatchers.Main) {
            for ((index, article) in articlesToRead.withIndex()) {
                currentIndex = index
                broadcastProgress(article)

                val textToSpeak = buildString {
                    append(article.title)
                    if (readBody && !article.description.isNullOrBlank()) {
                        append(". ")
                        append(article.description)
                    }
                }

                speakText(textToSpeak)
                // Mark article read in DB
                markArticleRead(article)
            }
            // Done reading
            broadcastDone()
            stopForeground(true)
            stopSelf()
        }
    }

    /**
     * Speaks text synchronously (blocks until done). We'll do a simple approach
     * using a callback. A more robust approach might queue the text with TTS.QUEUE_FLUSH
     */
    private suspend fun speakText(text: String) = suspendCancellableCoroutine<Unit> { cont ->
        val listener = TextToSpeech.OnUtteranceCompletedListener {
            // resume the coroutine
            if (!cont.isCancelled) {
                cont.resume(Unit) {}
            }
        }

        // For newer TTS versions, use setOnUtteranceProgressListener
        // Here we just do legacy:
        tts?.setOnUtteranceCompletedListener(listener)
        val params = hashMapOf<String, String>()
        params[TextToSpeech.Engine.KEY_PARAM_UTTERANCE_ID] = "TTS_UTTERANCE_ID"
        tts?.speak(text, TextToSpeech.QUEUE_FLUSH, params as Bundle?, "TTS_UTTERANCE_ID")
    }

    private fun markArticleRead(article: NewsArticle) {
        serviceScope.launch {
            val db = (application as KeyNewsApp).database
            db.newsArticleDao().markRead(article.link, true)
        }
    }

    private fun broadcastProgress(article: NewsArticle) {
        val intent = Intent(ACTION_READING_PROGRESS)
        intent.putExtra(EXTRA_ARTICLE_LINK, article.link)
        LocalBroadcastManager.getInstance(this).sendBroadcast(intent)

        // Also update our ongoing notification with the current article title
        updateNotification("Reading: ${article.title}")
    }

    private fun broadcastDone() {
        val intent = Intent(ACTION_READING_DONE)
        LocalBroadcastManager.getInstance(this).sendBroadcast(intent)
    }

    /**
     * Build or update the foreground notification
     */
    private fun buildNotification(info: String): Notification {
        return NotificationCompat.Builder(this, NOTIF_CHANNEL_ID)
            .setContentTitle("KeyNews TTS Reading")
            .setContentText(info)
            .setSmallIcon(android.R.drawable.ic_lock_silent_mode_off)
            .setOngoing(true)
            .build()
    }

    private fun updateNotification(info: String) {
        val notif = buildNotification(info)
        val nm = getSystemService(Context.NOTIFICATION_SERVICE) as NotificationManager
        nm.notify(NOTIF_ID, notif)
    }

    private fun createNotificationChannel() {
        val channel = NotificationChannel(
            NOTIF_CHANNEL_ID,
            "KeyNews TTS Reading",
            NotificationManager.IMPORTANCE_LOW
        )
        channel.description = "Background reading of news articles"
        val nm = getSystemService(Context.NOTIFICATION_SERVICE) as NotificationManager
        nm.createNotificationChannel(channel)
    }

    override fun onDestroy() {
        super.onDestroy()
        tts?.stop()
        tts?.shutdown()
        serviceJob.cancel()
    }

    override fun onBind(intent: Intent?): IBinder? {
        // We are not providing binding; return null
        return null
    }
}
