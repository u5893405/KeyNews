package com.example.keynews.ui.fetching

import com.example.keynews.data.model.NewsArticle
import org.jsoup.Jsoup
import java.util.UUID

object RssFetcher {

    // parseRss(...) returns a list of NewsArticle for the given sourceId
    // This is naive parsing for typical RSS
    fun parseRss(rssUrl: String, sourceId: Long): List<NewsArticle> {
        val result = mutableListOf<NewsArticle>()
        try {
            val doc = Jsoup.connect(rssUrl).timeout(5000).get()
            // Typically RSS has <item><title>...</title><link>...</link><description>...</description></item>
            val items = doc.select("item")
            for (item in items) {
                val title = item.select("title").first()?.text() ?: "No title"
                val link = item.select("link").first()?.text() ?: UUID.randomUUID().toString()
                val desc = item.select("description").first()?.text() ?: ""
                // publish date might be in <pubDate> ... we store epoch
                val pubDate = item.select("pubDate").first()?.text() ?: ""
                val epoch = System.currentTimeMillis() // for simplicity
                // You could parse pubDate string into an epoch if you want real accuracy

                val article = NewsArticle(
                    link = link,
                    sourceId = sourceId,
                    title = title,
                    description = desc,
                    publishDateUtc = epoch,
                    isRead = false
                )
                result.add(article)
            }
        } catch (e: Exception) {
            e.printStackTrace()
        }
        return result
    }
}
